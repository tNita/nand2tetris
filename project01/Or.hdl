/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    // Or â‰¡ NOT(NOT A AND NOT B)
    Not(in= a, out= nota);
    Not(in= b, out= notb);
    And(a= nota, b= notb, out= notaandb);
    Not(in= notaandb, out= out);

    //// Replace this comment with your code.
}